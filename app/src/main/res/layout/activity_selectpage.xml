<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/selectbackground"
    tools:context="com.example.dongdong.memorymaster.Selectpage">

    <RelativeLayout
        android:layout_width="370dp"
        android:layout_height="485dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="7dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <Button
            android:id="@+id/Level1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/Level2"
            android:layout_alignBottom="@+id/Level2"
            android:layout_alignStart="@+id/Level3"
            android:onClick="Play"
            android:text="Level1"
            tools:layout_editor_absoluteX="35dp"
            tools:layout_editor_absoluteY="39dp"
            android:background="@drawable/btn_selector" />

        <Button
            android:id="@+id/Level2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/Level4"
            android:layout_alignStart="@+id/Level4"
            android:layout_marginBottom="30dp"
            android:onClick="Play2"
            android:text="Level2" android:background="@drawable/btn_selector"/>

        <Button
            android:id="@+id/Level3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/Level4"
            android:layout_alignBottom="@+id/Level4"
            android:layout_alignStart="@+id/Level5"
            android:onClick="Play3"
            android:text="Level3"
            android:background="@drawable/btn_selector"/>

        <Button
            android:id="@+id/Level4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/Level6"
            android:layout_alignStart="@+id/Level6"
            android:layout_marginBottom="58dp"
            android:onClick="Play4"
            android:text="Level4"
            android:background="@drawable/btn_selector"
            />

        <Button
            android:id="@+id/Level5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/Level6"
            android:layout_alignBottom="@+id/Level6"
            android:layout_alignParentStart="true"
            android:layout_marginStart="58dp"
            android:onClick="Play5"
            android:text="Level5"
            android:background="@drawable/btn_selector"/>

        <Button
            android:id="@+id/Level6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="167dp"
            android:layout_marginStart="70dp"
            android:layout_toEndOf="@+id/Level5"
            android:onClick="Play6"
            android:text="Level6"
            android:background="@drawable/btn_selector"/>

        <Button
            android:id="@+id/Main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Level5"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="43dp"
            android:onClick="GoMain"
            android:text="Main"
            android:background="@drawable/btn_selector"/>

    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
